#include<iostream>
#include"stdafx.h"
#include"conio.h"
using namespace std;


int main() {
	float a;
	float b;
	float c;
	float rango;
	cout << "Ingrese A,B,C" << endl;
	cin >> a >> b >> c;
	if (a<=10 && a>=0) {
		if (b<=10 && b>=0) {
			if (c<=10 && c>=0) {
				rango = (a+b+c)/3;
			} else {
				cout << "c esta fuera de rango" << endl;
			}
		} else {
			cout << "b esta fuera de rango" << endl;
		}
	} else {
		cout << "a esta fuera de rango" << endl;
	}
	return 0;
}
